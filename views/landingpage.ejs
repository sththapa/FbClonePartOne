    <title>Facebook landing page</title>
</head>

<body>
    <div class="landingpagenavbar">
        <nav class="navbar navbar-expand-lg navbar-light bg-light" style="background-color: white  !important;">
            <img src="https://img.icons8.com/cute-clipart/64/000000/facebook-new.png"/>
           

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                
                <form class="form-inline my-2 my-lg-0">
                    <input class="form-control mr-sm-2" style=" border-radius: 40px;background-color: #F0F2F5;" type="search" placeholder="🔍Search Facebook" aria-label="Search">
                   
                </form>
                <div class="middlenav" style="width: 55%;">
                    <img src="https://img.icons8.com/material-rounded/24/000000/home.png">
                    <span><i class="fas fa-users"></i></span>
                    <span><i class="fas fa-headset"></i></span>


                </div>
                <div class="lastnav">

                    <img style="border-radius: 100%;" src="https://i.bollywoodmantra.com/albums/movie-stills/badmaash-company/shahid-kapur_anushka-sharma_meiyang-chang___189874.jpg" height="50px" width="50px" alt="">
                    <span>Shahid kapoor</span>
                    <span style="border-radius: 100%;height: 20px;width:20px;background-color:#D8DADF;padding: 10px;"><i class="fas fa-plus"></i></span>
                    <span style="border-radius: 100%;height: 20px;width:20px;background-color:#D8DADF;padding: 10px;"><i class="fab fa-facebook-messenger"></i></span>
                    <span style="border-radius: 100%;height: 20px;width:20px;background-color:#D8DADF;padding: 10px;"><i class="fas fa-bell"></i></span>
                    <span style="border-radius: 100%;height: 20px;width:20px;background-color:#D8DADF;padding: 10px;"><i class="fas fa-caret-down"></i></span>

                </div>
            </div>
        </nav>


    </div>

    <section style="background-color:#F0F2F5;">

        <div class="row">

            <div class="col-lg-4">
                <img src="https://img.icons8.com/color/48/000000/human-head.png"/>
                <span>Shahid Kapoor</span>
                <br>
                <img src="https://img.icons8.com/cute-clipart/64/000000/coronavirus.png"/>
                <span>Covid-19 Infromation Center</span>
                <br>
                <img src="https://img.icons8.com/wired/64/000000/friends.png"/>
                <span>Friends</span>
                <br>
                <img src="https://img.icons8.com/color/48/000000/facebook-messenger.png"/>
                <span>Messenger</span>
                <br>
                <img src="https://img.icons8.com/dusk/64/000000/flag-2.png"/>
                <span>Pages</span>
                <br>
                <i class="fas fa-caret-down"></i>
                <span>See More</span>
         
            </div>
            <div class="col-lg-4">

            </div>
            <div class="col-lg-4">

            </div>

        </div>


    </section>

    
